{
  "name": "nutrition-app",
  "description": "An application to plan a well balanced diet",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "private": true,
  "homepage": "http://JohnNilsson.github.io/nutrition",
  "dependencies": {
    "lodash-es": "4.17.20",
    "mobx": "6.0.4",
    "mobx-react-lite": "3.1.6",
    "mobx-state-tree": "^5.0.0",
    "mobx-utils": "^6.0.3",
    "rc-slider": "^9.7.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-window": "^1.8.6",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "yarn run build && gh-pages -d build",
    "prettier": "prettier --write 'public/**/*.html' 'src/**/*.[tj]s{x,}'"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^14.14.21",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "@types/react-window": "^1.8.2",
    "fibers": "^5.0.0",
    "gh-pages": "^3.1.0",
    "react-scripts": "^4.0.1",
    "typescript": "^4.1.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(lodash-es|rc-slider/es|rc-util/es|@babel/runtime/helpers/esm)/.*)"
    ],
    "moduleNameMapper": {
      "^.+.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "<rootDir>/tests/stub"
    }    
  }
}
