{
  "name": "swedish-food-composition-database",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "download-xml": "mkdir -p data && curl http://www7.slv.se/apilivsmedel/LivsmedelService.svc/Livsmedel/Naringsvarde/20220524 -o data/Naringsvarde.xml",
    "transform-xml-to-json": "ts-node --pretty --files transform 6NF.json < data/Naringsvarde.xml > data/Naringsvarde.6NF.json",
    "transform-xml-to-sql": "ts-node --pretty --files transform sql < data/Naringsvarde.xml > data/Naringsvarde.sql",
    "build": "npm run transform-xml-to-json",
    "test": "ts-node ./node_modules/tape/bin/tape src/**/*.test.ts"
  },
